generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Rank {
  BRONZE
  SILVER
  GOLD
}


model User{
    id          String      @id @default(uuid())
    name        String
    email       String      @unique
    password    String
    rank        String      @default("BRONZE")
    score       Int         @default(0)
    createdAt   DateTime    @default(now())
    updatedAt   DateTime   @updatedAt
}